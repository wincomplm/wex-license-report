<wex-definition>
    <meta>
        <group-id>com.wincomplm</group-id>   
        <artifact-id>wex-license-report</artifact-id>
        <display-name>License Report</display-name>
        <system>false</system>   
        <description>License Report</description>
    </meta>
   <build>
        <version>
            <major>${major}</major>
            <minor>${minor}</minor>
            <beta>${beta}</beta>
            <revision>${revision}</revision>
        </version>
        <host>
            <release>${wex.wt}</release>
        </host>
    </build>
    
    <install>
        <menus>
            <extension menu="header actions" file="navigation-actionModels" action="licenseReport"/>
        </menus>
        <actions>
            <page name="licenseReport" url="manager.jsp"/>  
        </actions>
    </install>
    
    <additional>
        <trial/>
        <timestamp>0</timestamp>
        <expires>0</expires>
        <client-id/>
        <client-name/>
        <user-id/>
    </additional>

</wex-definition>